{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@@*": ["components/*"],
      "@fe6/water-use": ["components/*"],
      "@fe6/water-use/*": ["components/*"]
    },
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "jsx": "preserve",
    "noImplicitAny": false,
    "target": "es6",
    "lib": ["dom", "es2017"],
    "skipLibCheck": true,
    "allowJs": true
  },
  "exclude": ["node_modules", "lib", "es", "antd-tools", "dist"]
}
